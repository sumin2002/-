      "source": [
        "16.1+2-3+4+5-6+7+8-9+...-99+100=?"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "5Z_dZajgHOwn",
        "outputId": "b8313ec8-de87-4648-b7b9-b7884088df85"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "1684\n"
          ]
        }
      ],
      "source": [
        "sum=0\n",
        "for i in range(1,101):\n",
        "  if i%3==0:\n",
        "    sum-=i\n",
        "  else:\n",
        "    sum+=i\n",
        "print(sum)"
      ]
    },
